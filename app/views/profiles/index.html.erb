<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Full name</th>
    <th>Contact number</th>
    <th>Date of birth</th>
    <th>User</th>
    <th>Slug</th>
    <th>Faceboo profile link</th>
    <th>Twitter profile link</th>
    <th>Google plus link</th>
    <th>Linked in profile link</th>
    <th>About me</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.full_name %></td>
    <td><%= profile.contact_number %></td>
    <td><%= profile.date_of_birth %></td>
    <td><%= profile.user_id %></td>
    <td><%= profile.slug %></td>
    <td><%= profile.faceboo_profile_link %></td>
    <td><%= profile.twitter_profile_link %></td>
    <td><%= profile.google_plus_link %></td>
    <td><%= profile.linked_in_profile_link %></td>
    <td><%= profile.about_me %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
